@model List<CoreLibrary.Models.Book>
@{
    Layout = null;
}

<!DOCTYPE html>
<div class="container">
    <div class="row">
        @foreach (var item in Model)
        {
            <a href="/Book/BookDetail/@item.Id">
                <img src="@item.ImageUrl" width=150 class="ml-5 mt-5" />
            </a>
        }
    </div>
</div>
